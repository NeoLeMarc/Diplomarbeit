    topToBottom(45)(BlockingQueueMANVEvent, MANVConnector);
    leftToRight.top(60)(BlockingQueueMANVEvent, BlockingQueueMANVCommand);
    drawObjects(BlockingQueueMANVEvent, BlockingQueueMANVCommand);

    % Threads
    Class.Thread("Thread")
