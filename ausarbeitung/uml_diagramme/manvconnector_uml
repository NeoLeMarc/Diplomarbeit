    % Corba
    Class.CorbaMessageContainer("CorbaMessageContainer")
            ()
            ("+send(serverIncoming: Incoming)");

    Class.CorbaDataMessageContainer("CorbaDataMessageContainer")
            ("-message: CORBA_DataMessage")
